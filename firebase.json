{
  "hosting": {
    "target": "app",
    "public": "public",
    "rewrites": [{
      "source": "/s/*",
      "destination": "/index.html"
    }],
    "headers": [{
      "source": "**",
      "headers": [{
        "key": "Cache-Control",
        "value": "no-cache"
      }]
    }],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**",
      "dev/**",
      "server.js"
    ]
  }
}
